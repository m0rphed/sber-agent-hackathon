{
    "dependencies": [
        "."
    ],
    "graphs": {
        "hybrid_agent": "./src/langgraph_app/graphs.py:hybrid",
        "rag": "./src/langgraph_app/graphs.py:rag"
    },
    "env": ".env",
    "python_version": "3.12",
    "dockerfile_lines": [
        "ENV PYTHONPATH=/deps/langgraph-app/src:$PYTHONPATH"
    ],
    "http": {
        "cors": {
            "allow_origins": [
                "*"
            ],
            "allow_methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "OPTIONS"
            ],
            "allow_headers": [
                "*"
            ],
            "allow_credentials": true
        },
        "configurable_headers": {
            "includes": [
                "x-user-id",
                "x-session-id"
            ],
            "excludes": [
                "authorization",
                "x-api-key"
            ]
        }
    },
    "store": {
        "ttl": {
            "default_ttl": 1440,
            "sweep_interval_minutes": 60,
            "refresh_on_read": true
        }
    },
    "checkpointer": {
        "ttl": {
            "default_ttl": 10080,
            "sweep_interval_minutes": 360
        }
    }
}